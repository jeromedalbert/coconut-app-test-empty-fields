<p id="notice"><%= notice %></p>

<h1>Coconuts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Diameter</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coconuts.each do |coconut| %>
      <tr>
        <td><%= coconut.name %></td>
        <td><%= coconut.diameter %></td>
        <td><%= coconut.weight %></td>
        <td><%= link_to 'Show', coconut %></td>
        <td><%= link_to 'Edit', edit_coconut_path(coconut) %></td>
        <td><%= link_to 'Destroy', coconut, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coconut', new_coconut_path %>
